    <Button fx:id="btIndietro" layoutX="70.0" layoutY="544.0" mnemonicParsing="false" onAction="#tornaSceltaRicerca" prefHeight="30.0" prefWidth="658.0" style="-fx-background-color: RED;" text="INDIETRO" textFill="WHITE">
        <font>
            <Font name="Georgia Pro" size="16.0" />
        </font>
    </Button>
    <TextField fx:id="tFcercaCV" layoutX="71.0" layoutY="270.0" prefHeight="26.0" prefWidth="658.0" promptText="nome centro vaccinale da cercare">
        <font>
            <Font name="Georgia Pro" size="16.0" />
        </font>
    </TextField>
    <Button fx:id="btCercaCV" layoutX="70.0" layoutY="501.0" mnemonicParsing="false" onAction="#cercaCVperNome" prefHeight="30.0" prefWidth="658.0" style="-fx-background-color: #598990;" text="CERCA IL CENTRO VACCINALE" textFill="WHITE">
