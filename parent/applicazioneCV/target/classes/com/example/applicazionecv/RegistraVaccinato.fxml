<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #ffffff;"
            xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.applicazionecv.GUI.ControllerRegistraVaccinato">
    <children>
        <ImageView fx:id="logoApp" fitHeight="303.0" fitWidth="311.0" layoutY="7.0" pickOnBounds="true"
                   preserveRatio="true" style="-fx-background-color: #ffffff;" AnchorPane.bottomAnchor="90.0"
                   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="297.0" AnchorPane.topAnchor="7.0">
            <image>
                <Image url="@IMM/logo.png"/>
            </image>
        </ImageView>
        <TextField fx:id="tFnomeV" layoutX="372.0" layoutY="80.0" prefHeight="26.0" prefWidth="194.0"
                   promptText="nome vaccinato"/>
        <Label layoutX="275.0" layoutY="85.0" text="Nome">
            <font>
                <Font name="Georgia Pro" size="14.0"/>
            </font>
        </Label>
        <Button fx:id="btRegistraVaccinato" layoutX="29.0" layoutY="294.0" mnemonicParsing="false"
                onAction="#registraVaccinato" prefHeight="30.0" prefWidth="548.0" style="-fx-background-color: #336e77;"
                text="REGISTRA" textFill="WHITE" AnchorPane.leftAnchor="29.0" AnchorPane.rightAnchor="23.0">
            <font>
                <Font name="Georgia Pro" size="16.0"/>
            </font>
        </Button>
        <Button fx:id="btIndietro" layoutX="29.0" layoutY="336.0" mnemonicParsing="false" onAction="#tornaCvRegistrato"
                prefHeight="30.0" prefWidth="548.0" style="-fx-background-color: RED;" text="INDIETRO" textFill="WHITE"
                AnchorPane.leftAnchor="29.0" AnchorPane.rightAnchor="23.0">
            <font>
                <Font name="Georgia Pro" size="16.0"/>
            </font>
        </Button>
        <Label layoutX="275.0" layoutY="135.0" text="Cognome">
            <font>
                <Font name="Georgia Pro" size="14.0"/>
            </font>
        </Label>
        <TextField fx:id="tFcognomeV" layoutX="371.0" layoutY="130.0" prefHeight="26.0" prefWidth="194.0"
                   promptText="cognome vaccinato"/>
        <Label layoutX="276.0" layoutY="186.0" text="Codice fiscale">
            <font>
                <Font name="Georgia Pro" size="14.0"/>
            </font>
        </Label>
        <TextField fx:id="tFcfV" layoutX="371.0" layoutY="181.0" prefHeight="26.0" prefWidth="193.0"
                   promptText="codice fiscale"/>
        <Label layoutX="275.0" layoutY="233.0" text="Tipologia">
            <font>
                <Font name="Georgia Pro" size="14.0"/>
            </font>
        </Label>
        <ComboBox fx:id="cbTipo" layoutX="372.0" layoutY="228.0" prefHeight="26.0" prefWidth="194.0">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="Pfizer"/>
                    <String fx:value="AstraZeneca"/>
                    <String fx:value="Moderna"/>
                    <String fx:value="J&amp;J"/>
                </FXCollections>
            </items>
        </ComboBox>
        <Label fx:id="lbErr" layoutX="31.0" layoutY="25.0" prefHeight="18.0" prefWidth="542.0" textFill="RED">
            <font>
                <Font name="Georgia Pro" size="14.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
